{
  "name": "really-less",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@turbo/gen": "1.9.7",
    "@types/node": "20.3.1",
    "eslint": "7.32.0",
    "eslint-config-custom": "*",
    "husky": "8.0.0",
    "lint-staged": "15.2.0",
    "prettier": "2.5.1",
    "tsc": "2.0.4",
    "turbo": "latest",
    "typescript": "5.1.3"
  },
  "lint-staged": {
    "*.{ts, md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
