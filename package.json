{
  "name": "really-less",
  "private": true,
  "workspaces": ["apps/*", "packages/*", "modules/*", "resolvers/*"],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "format": "yarn biome format --write . ",
    "lint": "turbo run lint",
    "prepare": "husky",
    "publish-packages": "turbo run build && changeset version && changeset publish",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/cli": "2.27.7",
    "@turbo/gen": "1.9.7",
    "@types/node": "20.3.1",
    "husky": "8.0.0",
    "lint-staged": "15.2.0",
    "tsc": "2.0.4",
    "turbo": "2.5.4",
    "typescript": "5.1.3"
  },
  "packageManager": "yarn@1.22.19"
}
